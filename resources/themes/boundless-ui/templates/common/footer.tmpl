{{ define "boundless-ui/footer" }}
    <!--尾部-->
    <footer class="footer">
        <div class="footer-line">
            <div class="icon-center">
                <span class="icon-box">
                    <a id="github-icon" target="_blank"
                       href="https://github.com/{{ .blogVars.Github }}"
                       class="foot-icon"></a>
                </span>
                <span class="icon-box">
                        <a id="qq-icon" target="_blank"
                           href="tencent://Message/?Uin={{ .blogVars.QQ }}&amp;websiteName=q-zone.qq.com&amp;Menu=yes"
                           class="foot-icon"></a>
                </span>
            </div>
        </div>
        {{ safe .blogVars.GlobalFooter }}
        <div class="footer-line">
            © 2022 - 2023 By {{ JoshuaZheng }}
        </div>
        <div class="footer-line">
            Theme BoundlessUI
        </div>
    </footer>
{{ end }}